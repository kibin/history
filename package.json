{
  "name": "@motorcycle/history",
  "version": "0.1.0",
  "description": "History Driver for Motorcycle.js",
  "main": "lib/index.js",
  "scripts": {
    "eslint": "eslint src/",
    "mocha": "mocha --compilers js:babel-core/register",
    "pretest-browser": "browserify test/browser/index.js -t babelify -o test/browser/page/tests-bundle.js",
    "test-browser": "testem",
    "posttest-browser": "rm test/browser/page/tests-bundle.js",
    "test": "npm run eslint && npm run test-browser",
    "compile-lib": "babel src/ -d lib/",
    "prepublish": "npm run test && npm run compile-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcyclejs/motorcycle-history.git"
  },
  "keywords": [
    "Motorcyclejs",
    "History"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motorcyclejs/motorcycle-history/issues"
  },
  "homepage": "https://github.com/motorcyclejs/motorcycle-history#readme",
  "peerDependencies": {
    "most": "*"
  },
  "dependencies": {
    "fast.js": "^0.1.1",
    "history": "^1.13.1",
    "most-subject": "^1.0.1"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-plugin-syntax-object-rest-spread": "^6.1.18",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.0.0",
    "eslint-config-cycle": "^3.1.0",
    "eslint-plugin-cycle": "^1.0.2",
    "eslint-plugin-no-class": "^0.1.0",
    "mocha": "^2.3.4",
    "most": "^0.16.0",
    "testem": "^0.9.11"
  }
}
